{
  "name": "magicjar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "kea": "^2.3.7",
    "kea-loaders": "^0.4.0",
    "kea-typegen": "^0.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-grid-system": "^7.1.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "reselect": "^4.0.0",
    "sass": "^1.32.11",
    "typescript": "^4.2.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "concurrently \"yarn run start:react\" \"yarn run start:typegen\" -n REACT,TYPEGEN -c blue,green",
    "start:react": "react-scripts start",
    "start:typegen": "kea-typegen watch",
    "build": "yarn run build:typegen && yarn run build:react",
    "build:react": "react-scripts build",
    "build:typegen": "kea-typegen write",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "yarn eslint src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,yml,css,scss}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --fix --max-warnings=0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.0.2",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
